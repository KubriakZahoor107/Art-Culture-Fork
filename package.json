{
  "name": "Art-Culture-Fork",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite --config vite.config.client.js",
    "dev:ssr": "node server/app.js",
    "build": "yarn prisma:generate && vite build --config vite.config.client.js && vite build --config vite.config.server.js",
    "preview": "node server/app.js",
    "start": "node server/app.js",
    "prisma:generate": "prisma generate --schema=server/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=server/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=server/prisma/schema.prisma",
    "postinstall": "yarn prisma:generate"
  },
  "prisma": {
    "schema": "server/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "cheerio": "1.0.0",
    "cors": "^2.8.5",
    "dom-serializer": "2.0.0",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "flat-cache": "^6.1.8",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "nodemailer": "^6.10.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.6.0",
    "react-router-dom": "^7.6.0",
    "sharp": "^0.34.1",
    "vite": "^4.4.9",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "latest",
    "prisma": "^6.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}